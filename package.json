  "devDependencies": {
    "babel-core": "6.8.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "css-loader": "0.25.0",
    "eslint": "3.12.2",
    "eslint-config-google": "0.7.1",
    "eslint-plugin-react": "6.8.0",
    "gh-pages": "1.2.0",
    "hjs-webpack": "8.1.0",
    "html-loader": "0.4.4",
    "image-size": "0.5.0",
    "json-loader": "0.5.4",
    "lodash.merge": "4.6.0",
    "markdown-loader": "0.1.7",
    "node-sass": "3.7.0",
    "postcss-loader": "0.13.0",
    "prettier": "1.14.3",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "surge": "0.20.1",
    "webpack": "1.13.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "arrowParens": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "google"
    ],
    "rules": {
      "max-len": 0,
      "require-jsdoc": 0
    },
    "env": {
      "browser": true
    },
    "globals": {
      "require": true
    }
  },
  "scripts": {
    "lint": "eslint ./src --ignore-pattern /src/vendor/*",
    "test": "echo \"Error: no test specified.\" && exit 0",
    "posttest": "npm run lint",
    "build": "NODE_ENV=production webpack",
    "build:nofollow": "echo 'User-agent: *\nDisallow: /' > ./build/robots.txt",
    "start": "NODE_ENV=development hjs-dev-server",
    "prestage": "npm run build && npm run build:nofollow",
    "stage": "surge ./build googlefonts-japanese.kennethormandy.com",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d ./build"
  }
}
